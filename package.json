{
  "name": "laserbat-ast",
  "version": "0.0.1",
  "description": "constructors for the LaserBat AST format",
  "author": "Shape Security Labs",
  "homepage": "https://github.com/shapesecurity/laserbat-ast-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/shapesecurity/laserbat-ast-js.git"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "mocha --compilers js:6to5/lib/6to5/register --inline-diffs --check-leaks --ui tdd --reporter dot test",
    "build": "mkdir lib; printf '// Generated by 6to5 version %b\\n' $(6to5 --version) >lib/index.js && 6to5 src/index.js >>lib/index.js"
  },
  "devDependencies": {
    "6to5": "^1.14.14",
    "mocha": "^2.0.1"
  },
  "keywords": [
    "LaserBat",
    "AST",
    "node",
    "constructor",
    "class",
    "laser",
    "bat",
    "abstract",
    "syntax",
    "tree"
  ],
  "bugs": {
    "url": "https://github.com/shapesecurity/laserbat-ast-js/issues"
  },
  "license": "Apache-2.0"
}
